{
  "name": "react-redux-functional-boilerplate",
  "version": "0.1.2",
  "description": "a Functional React/Redux webpack boilerplate",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8080 --host 0.0.0.0 --progress --colors --content-base build/",
    "dev-prod": "NODE_ENV=production webpack-dev-server --config webpack.config.js --port 8080 --host 0.0.0.0 --progress --colors --content-base build/",
    "build": "NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "build-dev": "NODE_ENV=development webpack --config webpack.config.js --progress --colors",
    "clean": "./node_modules/.bin/rimraf ./build && echo --- Cleaned ---"
  },
  "author": "Guillaume ARM",
  "dependencies": {
    "ramda": "^0.22.1",
    "ramda-fantasy": "^0.6.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "reselect": "^2.5.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.3",
    "html-webpack-plugin": "^2.21.0",
    "react-hot-loader": "^3.0.0-alpha.3",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
