{
  "name": "react-redux-functional-boilerplate",
  "version": "0.6.0",
  "description": "a Functional React/Redux webpack boilerplate",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "build:dev": "NODE_ENV=development webpack --progress",
    "build:all": "npm run -s build ; npm run -s build:dev",
    "dev": "NODE_ENV=development webpack-dev-server --progress",
    "dev:prod": "NODE_ENV=production webpack-dev-server --progress",
    "test": "npm run -s clean ; npm run -s build:all",
    "preversion": "npm run -s test",
    "clean": "rimraf ./dist",
    "open": "opn 'http://localhost:8080'"
  },
  "author": {
    "name": "Guillaume ARM",
    "email": "guillaume.arm@redpelicans.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/guillaumearm/react-redux-functional-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/guillaumearm/react-redux-functional-boilerplate/issues"
  },
  "homepage": "https://github.com/guillaumearm/react-redux-functional-boilerplate/issues",
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "functional",
    "boilerplate",
    "ramda"
  ],
  "dependencies": {
    "keymirror": "^0.1.1",
    "ramda": "^0.22.1",
    "ramda-fantasy": "^0.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-ramda": "^1.1.6",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^3.12.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "opn-cli": "^3.1.0",
    "react-hot-loader": "^3.0.0-alpha.3",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  }
}
