{
  "name": "react-redux-functional-boilerplate",
  "version": "0.6.0",
  "description": "a Functional React/Redux webpack boilerplate",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8080 --host 0.0.0.0 --progress --colors --content-base build/",
    "dev-prod": "NODE_ENV=production webpack-dev-server --config webpack.config.js --port 8080 --host 0.0.0.0 --progress --colors --content-base build/",
    "build": "NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "build-dev": "NODE_ENV=development webpack --config webpack.config.js --progress --colors",
    "clean": "rimraf ./build && echo --- Cleaned ---",
    "open": "opn 'http://localhost:8080'"
  },
  "author": "Guillaume ARM",
  "dependencies": {
    "keymirror": "^0.1.1",
    "ramda": "^0.22.1",
    "ramda-fantasy": "^0.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-ramda": "^1.1.6",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^3.12.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "opn-cli": "^3.1.0",
    "react-hot-loader": "^3.0.0-alpha.3",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  }
}
