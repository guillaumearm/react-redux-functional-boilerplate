{
  "name": "react-redux-functional-boilerplate",
  "version": "0.7.0",
  "description": "a Functional React/Redux webpack boilerplate",
  "config": {
    "host": "127.0.0.1",
    "port": 3042
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "build:dev": "NODE_ENV=development webpack --progress",
    "build:all": "npm run -s build ; npm run -s build:dev",
    "dev": "NODE_ENV=development node -r babel-register server.js --progress",
    "dev:prod": "NODE_ENV=production node -r babel-register server.js --progress",
    "dev:electron": "NODE_ENV=development node -r babel-register server.js --electron",
    "dev:prod:electron": "NODE_ENV=production node -r babel-register server.js --electron",
    "test": "jest",
    "test:all": "npm run -s clean ; npm run -s build:all ; npm run -s test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "preversion": "npm run -s test",
    "clean": "rimraf ./dist",
    "open": "node scripts/open.js"
  },
  "jest": {
    "globals": {
      "process.env.NODE_ENV": "test",
      "__NODE_ENV__": "test",
      "__DEVELOPMENT__": false,
      "__PRODUCTION__": false,
      "__TEST__": true
    },
    "testEnvironment": "node",
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "author": {
    "name": "Guillaume ARM",
    "email": "guillaume.arm@redpelicans.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/guillaumearm/react-redux-functional-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/guillaumearm/react-redux-functional-boilerplate/issues"
  },
  "homepage": "https://github.com/guillaumearm/react-redux-functional-boilerplate/issues",
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "functional",
    "boilerplate",
    "ramda"
  ],
  "dependencies": {
    "keymirror": "^0.1.1",
    "ramda": "^0.22.1",
    "ramda-fantasy": "^0.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-ramda": "^1.1.6",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "electron": "^1.4.13",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^3.12.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^18.0.0",
    "opn": "^4.0.2",
    "react-hot-loader": "^3.0.0-alpha.3",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-target-electron-renderer": "^0.4.0",
    "yargs": "^6.5.0"
  }
}
